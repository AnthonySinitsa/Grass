#pragma kernel CSMain

struct GrassData
{
    float3 position;
    float2 uv;
};

RWStructuredBuffer<GrassData> grassBuffer;

[numthreads(10, 1, 1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    GrassData data = grassBuffer[id.x];
    // Here you can manipulate grass data if needed
    grassBuffer[id.x] = data;
}
